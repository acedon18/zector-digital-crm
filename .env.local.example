# Local Development Environment Variables
# Copy this file to .env.local and fill in your actual values

# MongoDB Connection String
# Get this from your MongoDB Atlas dashboard or local MongoDB instance
MONGO_URI=mongodb+srv://username:password@cluster.mongodb.net/database_name?retryWrites=true&w=majority

# JWT Secret for Authentication
# Generate a random secret key for development
JWT_SECRET=your_development_jwt_secret_key_here

# Data Enrichment API Keys (for development testing)
VITE_CLEARBIT_API_KEY=your_clearbit_api_key_here
VITE_HUNTER_API_KEY=your_hunter_io_api_key_here
VITE_IPINFO_TOKEN=your_ipinfo_token_here

# Use real data in development
VITE_USE_REAL_DATA=true
VITE_API_ENDPOINT=http://localhost:3000
