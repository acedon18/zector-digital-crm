{
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "buildCommand": "cd pulse-main && npm install && npm run build || (echo 'Build failed' && ls -la)",
  "outputDirectory": "pulse-main/dist",
  "installCommand": "echo 'Skipping root install'",
  "framework": null,
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
