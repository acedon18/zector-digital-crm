{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/track",
      "dest": "/api/track.js"
    },
    {
      "src": "/api/companies",
      "dest": "/api/companies.js"
    },
    {
      "src": "/api/discover-leads",
      "dest": "/api/discover-leads.js"
    },
    {
      "src": "/api/visitors",
      "dest": "/api/visitors.js"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/api/test",
      "dest": "/api/test.js"
    },
    {
      "src": "/api/test-es",
      "dest": "/api/test-es.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],  "env": {
    "VITE_USE_REAL_DATA": "true"
  }
}
