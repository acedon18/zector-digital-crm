    },
    {
      "src": "/api/env-check",
      "dest": "/api/env-check.js"
    },
    {
      "src": "/api/database-status",
      "dest": "/api/database-status.js"
    },
    {
      "src": "/api/enrich",
      "dest": "/api/enrich.js"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/api/hello",
      "dest": "/api/hello.js"
    },
    {
      "src": "/api/debug-uri",
      "dest": "/api/debug-uri.js"
    },
    {
      "src": "/api/test-connections",
      "dest": "/api/test-connections.js"
    },
    {
      "src": "/api/visitors",
      "dest": "/api/visitors.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_USE_REAL_DATA": "true"
  }
}
