# Environment Configuration for Zector Digital Leads CRM

# Real Data Configuration
VITE_USE_REAL_DATA=true
VITE_API_ENDPOINT=https://api.zectordigital.com

# Data Enrichment API Keys (get these from the respective providers)
VITE_CLEARBIT_API_KEY=your_clearbit_api_key_here
VITE_HUNTER_API_KEY=your_hunter_io_api_key_here
VITE_IPINFO_TOKEN=your_ipinfo_token_here

# Alternative IP Geolocation Services (choose one)
VITE_IPAPI_KEY=your_ipapi_key_here
VITE_ABSTRACT_API_KEY=your_abstract_api_key_here

# Google Services (for enhanced data)
VITE_GOOGLE_PLACES_API_KEY=your_google_places_api_key_here

# Platform API Keys for Lead Discovery
VITE_GOOGLE_ADS_CLIENT_ID=your_google_ads_client_id
VITE_GOOGLE_ADS_CLIENT_SECRET=your_google_ads_client_secret
VITE_META_ADS_ACCESS_TOKEN=your_meta_ads_access_token
VITE_TIKTOK_ADS_ACCESS_TOKEN=your_tiktok_ads_access_token

# Database Configuration (for storing real visitor data)
VITE_DATABASE_URL=your_database_connection_string

# GDPR and Privacy Settings
VITE_GDPR_COMPLIANT=true
VITE_ANONYMIZE_IPS=true
VITE_DATA_RETENTION_DAYS=365
